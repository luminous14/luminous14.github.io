html {
  background-color:white;
  font-family: Helvetica Neue;
}

.myname {
  font-size:400%;
  color: #636363;
  text-align: center;
}

div:first-child {
    text-align: center;
}

div:first-child a {
  text-decoration: none;
}

div:nth-child(3) {
